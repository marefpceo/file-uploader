<header>
  <div>
    <h1>File Uploader</h1>
    <% if (user !== null) { %>
      <p>Welcome <%= user %></p>
    <% } %>
  </div>
  <% if (user !== null) { %>
    <form action='/logout' method='post'>
      <button type='submit' name='logout-btn' id='logout-btn'>Logout</button>
    </form>
  <% } %>
</header>